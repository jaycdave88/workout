!!!
%html
%head
  %title Workout Application
  = stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true
  = javascript_include_tag 'application', 'data-turbolinks-track' => true
  = csrf_meta_tags
%body
  %nav.navbar.navbar-default
    .container
      .navbar-header
        = link_to "Workout Tracker", root_path, class: "navbar-brand"
      .nav.navbar-nav.navbar-right
        - if user_signed_in? 
          = link_to "New Workout", new_workout_path, class: "btn btn-primary navbar-btn"
          = link_to "Log Out", destroy_user_session_path, :method => :delete ,class: "btn btn-default navbar-btn"
  .container
    %p.notice= notice 
    %p.alert= alert 
    = yield


     
